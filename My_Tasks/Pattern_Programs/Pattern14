/*

Enter the No of Rows: 5

A
A B
A B C
A B C D

 */

import java.util.Scanner;
public class Pattern14 {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        System.out.print("Enter the No of Rows: ");
        int n= sc.nextInt();
        for(int i=0;i<n;i++){
            for(char ch='A';ch<='A'+i;ch++){
                System.out.print(ch+" ");
            }
            System.out.println();
        }
    }
}
